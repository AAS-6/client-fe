model regency {
  id         Int        @id(map: "Regency_pkey") @default(autoincrement())
  name       String
  provinceId Int
  address    address[]
  district   district[]
  province   province   @relation(fields: [provinceId], references: [id], onUpdate: NoAction)

  @@schema("public")
}

